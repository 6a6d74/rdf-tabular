@prefix : <http://example.org/countries.csv#> .
@prefix slice: <http://example.org/country_slice.csv#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:countryCode a rdf:Property;
   rdfs:label "countryCode"; .

:latitude a rdf:Property;
   rdfs:label "latitude" .

:longitude a rdf:Property;
   rdfs:label "longitude" .

:name a rdf:Property;
   rdfs:label "name" .

slice:countryRef a rdf:Property;
   rdfs:label "countryRef" .

slice:population a rdf:Property;
   rdfs:label "population" .

slice:year a rdf:Property;
   rdfs:label "year" .

:AD :countryCode "AD";
   :latitude 42.546245;
   :longitude 1.601554;
   :name "Andorra" .

:AE :countryCode "AE";
   :latitude 23.424076;
   :longitude 53.847818;
   :name "United Arab Emirates" .

:AF :countryCode "AF";
   :latitude 33.93911;
   :longitude 67.709953;
   :name "Afghanistan" .

[
   a csvw:TableGroup;
   csvw:table :table, slice:table
] .

:table a csvw:Table;
   csvw:row :AD, :AE, :AF;
   dcat:distribution [
     a dcat:Distribution;
     dcat:downloadURL <http://example.org/countries.csv>
   ] .

slice:table a csvw:Table;
   csvw:row [
     slice:countryRef "AF";
     slice:population 9616353;
     slice:year "1960"^^xsd:gYear
   ],  [
     slice:countryRef "AF";
     slice:population 9799379;
     slice:year "1961"^^xsd:gYear
   ],  [
     slice:countryRef "AF";
     slice:population 9989846;
     slice:year "1962"^^xsd:gYear
   ];
   dcat:distribution [
     a dcat:Distribution;
     dcat:downloadURL <http://example.org/country_slice.csv>
   ] .
