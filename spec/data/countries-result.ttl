@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/countries.csv#AD> schema:latitude 42.546245;
   schema:longitude 1.601554;
   schema:name "Andorra";
   <http://www.geonames.org/ontology#countryCode> "AD";
   csvw:rownum 2 .

<http://example.org/countries.csv#AE> schema:latitude 23.424076;
   schema:longitude 53.847818;
   schema:name "United Arab Emirates";
   <http://www.geonames.org/ontology#countryCode> "AE";
   csvw:rownum 3 .

<http://example.org/countries.csv#table> a csvw:Table;
   csvw:row <http://example.org/countries.csv#AD>,
     <http://example.org/countries.csv#AE>,
     <http://example.org/countries.csv#AF>;
   csvw:tableSchema [
     a csvw:Schema;
     csvw:column [
       a csvw:Column;
       csvw:aboutUrl "http://example.org/countries.csv{#countryCode}";
       csvw:colnum 1;
       csvw:propertyUrl "http://www.geonames.org/ontology{#_name}"
     ],  [
       a csvw:Column;
       csvw:aboutUrl "http://example.org/countries.csv{#countryCode}";
       csvw:colnum 2;
       csvw:propertyUrl "http://schema.org/{_name}"
     ],  [
       a csvw:Column;
       csvw:aboutUrl "http://example.org/countries.csv{#countryCode}";
       csvw:colnum 3;
       csvw:propertyUrl "http://schema.org/{_name}"
     ],  [
       a csvw:Column;
       csvw:aboutUrl "http://example.org/countries.csv{#countryCode}";
       csvw:colnum 4;
       csvw:propertyUrl "http://schema.org/{_name}"
     ]
   ];
   dcat:distribution [
     a dcat:Distribution;
     dcat:downloadURL <http://example.org/countries.csv>
   ] .

<http://example.org/country_slice.csv#table> a csvw:Table;
   csvw:row [
     <http://example.org/country_slice.csv#countryRef> <http://example.org/countries.csv#AF>;
     <http://example.org/country_slice.csv#population> 9616353;
     <http://example.org/country_slice.csv#year> "1960"^^xsd:gYear;
     csvw:rownum 2
   ],  [
     <http://example.org/country_slice.csv#countryRef> <http://example.org/countries.csv#AF>;
     <http://example.org/country_slice.csv#population> 9799379;
     <http://example.org/country_slice.csv#year> "1961"^^xsd:gYear;
     csvw:rownum 3
   ],  [
     <http://example.org/country_slice.csv#countryRef> <http://example.org/countries.csv#AF>;
     <http://example.org/country_slice.csv#population> 9989846;
     <http://example.org/country_slice.csv#year> "1962"^^xsd:gYear;
     csvw:rownum 4
   ];
   csvw:tableSchema [
     a csvw:Schema;
     csvw:column [
       a csvw:Column;
       csvw:colnum 1;
       csvw:propertyUrl "{#_name}";
       csvw:valueUrl "http://example.org/countries.csv{#countryRef}"
     ],  [
       a csvw:Column;
       csvw:colnum 2;
       csvw:propertyUrl "{#_name}"
     ],  [
       a csvw:Column;
       csvw:colnum 3;
       csvw:propertyUrl "{#_name}"
     ]
   ];
   dcat:distribution [
     a dcat:Distribution;
     dcat:downloadURL <http://example.org/country_slice.csv>
   ] .

<http://example.org/countries.csv#AF> schema:latitude 33.93911;
   schema:longitude 67.709953;
   schema:name "Afghanistan";
   <http://www.geonames.org/ontology#countryCode> "AF";
   csvw:rownum 4 .

 [
    a csvw:TableGroup;
    csvw:table <http://example.org/countries.csv#table>,
      <http://example.org/country_slice.csv#table>
 ] .
