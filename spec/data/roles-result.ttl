   rdfs:label "Payscale Maximum (£)" .

<http://example.org/junior-roles.csv#Payscale%20Minimum%20(%C2%A3)> a rdf:Property;
   rdfs:label "Payscale Minimum (£)" .

j:Profession a rdf:Property;
